/dts-v1/;

#include "imx6dl.dtsi"
#include "imx6qdl-nicore8.dtsi"

/ {
	model = "Logos i.MX6 Solo NiCore8 evaluation board";
	compatible = "logos,imx6dl-nicore8", "fsl,imx6dl";
};

/* USDHC1, SD Card */
&usdhc1 {
        pinctrl-names = "default";
        bus-width = <4>;
        pinctrl-0 = <&pinctrl_usdhc1>;
        vmmc-supply = <&reg_sdio_pwr>;
        cd-gpios = <&gpio6 14 GPIO_ACTIVE_LOW>;
	no-1-8-v;
        status = "okay";
};

/* USDHC3, eMMC */
&usdhc3 {
        pinctrl-names = "default";
        bus-width = <8>;
        pinctrl-0 = <&pinctrl_usdhc3>;
        vmmc-supply = <&reg_3p3v>;
        non-removable;
        status = "okay";
};
